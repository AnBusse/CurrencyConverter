<html>
<head>
    {% block head %}
        <title>{% block title %}{% endblock %} - My Webpage</title>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <link rel="stylesheet" href="{{ asset('css/main.css') }}" />
    {% endblock %}
</head>
<body>

<!-- Header -->
<header id="header" class="alt">
        <div class="inner">
                <h1>CurrencyConverter</h1>
                <p>A free currency converter based on the fixer API</p>
        </div>
</header>

<!-- Wrapper -->
<div id="wrapper">

        <!-- Banner -->
        <section id="intro" class="main">
            {% block home_content %}{% endblock %}
        </section>

        <footer id="footer">
                {% block footer %}
                <ul class="icons">
                        <li><a href="#" class="icon fa-twitter"><span class="label">Twitter</span></a></li>
                        <li><a href="#" class="icon fa-facebook"><span class="label">Facebook</span></a></li>
                        <li><a href="#" class="icon fa-instagram"><span class="label">Instagram</span></a></li>
                        <li><a href="#" class="icon fa-linkedin"><span class="label">LinkedIn</span></a></li>
                        <li><a href="#" class="icon fa-envelope"><span class="label">Email</span></a></li>
                </ul>
                <p class="copyright">&copy; Untitled. Design: <a href="https://templated.co">TEMPLATED</a>. Images: <a href="https://unsplash.com">Unsplash</a>.</p>
                {% endblock %}
        </footer>

</div>

<!-- Scripts -->
<script src="assets/js/jquery.min.js"></script>
<script src="assets/js/skel.min.js"></script>
<script src="assets/js/util.js"></script>
<script src="assets/js/main.js"></script>

</body>

</html>